/* HomelessHelper - ackit */

/* validate number */
'/^([0-9]{3}[\-]?[0-9]{3}[\-]?[0-9]{4})$/'

/* validate name */
  /* first */ /* last */
    '/^([A-Za-z]{2,30})$/'

/* email */
'/^[A-Za-z0-9][\S]+\@[\S]+\.[A-Za-z]{2,3}$/'

/* dob yyyy-mm-dd */
'/^[0-9]{4}-[0-9]{1,2}-[0-9]{1,2}$/'

/* username */
    '/^([A-Za-z][A-Za-z0-9]{4,20})$/'
    
/* password */
   '/^([A-Za-z0-9\!\@\#\$\%\^\&\*\(\)\`\~\{\}\}\'\"\;\?\.\,]{7,20})$/'
    
    
    
    
    
    
    
    
    
